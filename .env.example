# Copy this file to .env.local and replace placeholders with real credentials.
# Do not commit actual secrets.

# Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Supabase optional toggles
NEXT_PUBLIC_SUPABASE_STORAGE_BUCKET=clothing
NEXT_PUBLIC_SUPABASE_STORAGE_FOLDER=uploads

# Mistral AI API
MISTRAL_API_KEY=your_mistral_api_key
MISTRAL_IMAGE_API_KEY=your_mistral_image_api_key
MISTRAL_AGENT_ID=your_mistral_image_agent_id

# Weather
ACCUWEATHER_API_KEY=your_accuweather_api_key
NEXT_PUBLIC_IPINFO_API_KEY=your_ipinfo_api_key


# Sightengine
SIGHTENGINE_API_USER=your_sightengine_user
SIGHTENGINE_API_SECRET=your_sightengine_secret
